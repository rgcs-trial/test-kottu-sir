{
  "name": "restaurant-saas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "optimize-build": "tsx scripts/optimize-build.ts",
    "analyze": "ANALYZE=true npm run build",
    "bundle-analyzer": "npm run analyze && npx webpack-bundle-analyzer .next/static/webpack-stats.json",
    "performance-test": "tsx scripts/performance-test.ts",
    "cache-test": "tsx scripts/cache-test.ts",
    "build:cloudflare": "npm run lint && npm run type-check && next build && npx @opennextjs/cloudflare",
    "build:production": "npm run build:cloudflare",
    "build:staging": "npm run build:cloudflare",
    "build:preview": "npm run build:cloudflare",
    "deploy:production": "npm run build:production && wrangler deploy --env production",
    "deploy:staging": "npm run build:staging && wrangler deploy --env staging",
    "deploy:preview": "npm run build:preview && wrangler deploy --env development",
    "preview:local": "npm run build:cloudflare && wrangler dev",
    "preview:remote": "npm run build:cloudflare && wrangler dev --remote",
    "preview:staging": "npm run build:staging && wrangler dev --env staging --remote",
    "validate:production": "tsx scripts/validate-deployment.ts --env production",
    "validate:staging": "tsx scripts/validate-deployment.ts --env staging",
    "validate:preview": "tsx scripts/validate-deployment.ts --env development",
    "setup:cloudflare": "tsx scripts/setup-cloudflare.ts",
    "setup:secrets": "tsx scripts/setup-secrets.ts",
    "setup:env": "tsx scripts/setup-environment.ts",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "cf-login": "wrangler auth login",
    "cf-whoami": "wrangler auth whoami",
    "cf-logout": "wrangler auth logout",
    "kv:create": "tsx scripts/kv-setup.ts",
    "r2:create": "tsx scripts/r2-setup.ts",
    "do:create": "tsx scripts/durable-objects-setup.ts",
    "queues:create": "tsx scripts/queues-setup.ts",
    "logs:production": "wrangler tail --env production",
    "logs:staging": "wrangler tail --env staging",
    "logs:development": "wrangler tail --env development",
    "analytics:production": "wrangler analytics --env production",
    "analytics:staging": "wrangler analytics --env staging",
    "test:integration": "tsx scripts/test-integration.ts",
    "test:deployment": "tsx scripts/test-deployment.ts",
    "test:performance": "tsx scripts/test-performance.ts",
    "rollback:production": "tsx scripts/rollback.ts --env production",
    "rollback:staging": "tsx scripts/rollback.ts --env staging",
    "health:check": "tsx scripts/health-check.ts",
    "smoke:test": "tsx scripts/smoke-test.ts",
    "secrets:rotate": "tsx scripts/rotate-secrets.ts",
    "maintenance:mode": "tsx scripts/maintenance-mode.ts",
    "deploy": "npm run deploy:production",
    "pages:build": "npm run build:cloudflare",
    "preview": "npm run preview:local"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-email/components": "^0.5.0",
    "@react-email/render": "^1.2.0",
    "@react-email/tailwind": "^1.2.2",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.38.5",
    "@types/uuid": "^9.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "next": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-email": "^4.2.8",
    "react-hook-form": "^7.48.2",
    "resend": "^6.0.1",
    "stripe": "^14.9.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.8.4",
    "@cloudflare/workers-types": "^4.20231218.0",
    "@opennextjs/cloudflare": "^1.0.0-beta.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.6.0",
    "typescript": "^5.3.3",
    "vercel": "latest",
    "webpack-bundle-analyzer": "^4.9.1",
    "wrangler": "^3.22.1"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
